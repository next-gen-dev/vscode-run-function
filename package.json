{
    "name": "vscode-run-function",
    "displayName": "Run Function",
    "description": "Run JavaScript and TypeScript functions with one click!",
    "version": "0.1.1",
    "publisher": "nextgendev",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/next-gen-dev/vscode-run-function"
    },
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Debuggers",
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/*.{mjs,ts,tsx}"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "title": "Enable Run Function CodeLens",
                "command": "run-function.enableCodeLens",
                "category": "Run Function"
            },
            {
                "title": "Disable Run Function CodeLens",
                "command": "run-function.disableCodeLens",
                "category": "Run Function"
            }
        ],
        "configuration": {
            "properties": {
                "run-function.enableCodeLens": {
                    "type": "boolean",
                    "default": true
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "package": "vsce package",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/vscode": "^1.62.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "@vscode/test-electron": "^1.6.2"
    },
    "dependencies": {
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
}